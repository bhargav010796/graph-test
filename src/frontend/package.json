{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@stomp/stompjs": "^6.1.2",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^0.24.0",
    "cors": "^2.8.5",
    "dygraphs": "^2.1.0",
    "env-cmd": "^10.1.0",
    "jquery": "^3.6.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-scripts": "^5.0.1",
    "sockjs": "^0.3.24",
    "sockjs-client": "^1.6.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "test": "env-cmd -f .env.development react-scripts test",
    "eject": "react-scripts eject",
    "start:dev": "env-cmd -f .env.development react-scripts start",
    "build:staging": "env-cmd -f .env.staging npm run-script build",
    "build:qa": "env-cmd -f .env.qa npm run-script build",
    "build:production": "env-cmd -f .env.production npm run build-css && react-scripts build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:8080"
}
